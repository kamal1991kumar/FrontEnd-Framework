{
    "source": "./src",
    "includes": [ "\\.js$", "\\.jsx$" ],
    "excludes": [ "^\\.dump", "\/constants\/" ],
    "destination": "./docs",
    "plugins": [
        {
            "name": "esdoc-standard-plugin"
        },
        {
            "name": "esdoc-jsx-plugin",
            "option": { "enable": true }
        },
        {
            "name": "esdoc-standard-plugin"
        },
        {
            "name": "esdoc-react-plugin"
        }
    ]
}